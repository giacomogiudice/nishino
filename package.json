{
  "name": "nishino",
  "version": "1.0.0",
  "description": "A small reader for Tomotoshi Nishino's selection of papers on tensor networks",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run lint",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "prettier --check --plugin-search-dir=. --ignore-path .gitignore . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. --ignore-path .gitignore .",
    "db-update": "node functions/util/setup.js"
  },
  "author": "Giacomo Giudice",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "autoprefixer": "^10.4.7",
    "cheerio": "^1.0.0-rc.12",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.14.1",
    "fuse.js": "^6.6.2",
    "glob": "^9.3.2",
    "html-entities": "^2.3.3",
    "katex": "^0.16.0",
    "node-fetch": "^3.2.6",
    "postcss": "^8.4.21",
    "posthtml": "^0.16.6",
    "posthtml-include": "^1.7.4",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.1",
    "redis": "^4.6.5",
    "sass": "^1.53.0",
    "svelte": "^3.48.0",
    "svelte-preprocess": "^5.0.3",
    "vite": "^4.3.9"
  },
  "browserslist": [
    "last 3 version",
    "> 0.25%",
    "not dead"
  ]
}
