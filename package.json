{
  "name": "nishino",
  "version": "1.0.0",
  "description": "A small reader for Tomotoshi Nishino's selection of papers on tensor networks",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run lint",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "prettier --check --plugin-search-dir=. --ignore-path .gitignore . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. --ignore-path .gitignore ."
  },
  "author": "Giacomo Giudice",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.6",
    "glob": "^7.1.7",
    "html-entities": "^2.3.2",
    "katex": "^0.13.6",
    "node-fetch": "^2.6.1",
    "svelte": "^3.38.2"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.9",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "postcss": "^8.2.15",
    "posthtml": "^0.15.2",
    "posthtml-include": "^1.7.1",
    "prettier": "^2.3.0",
    "prettier-plugin-svelte": "^2.3.0",
    "sass": "^1.32.12",
    "svelte-preprocess": "^4.7.3",
    "vite": "^2.3.2"
  },
  "browserslist": [
    "last 3 version",
    "> 0.25%",
    "not dead"
  ]
}
