{
  "name": "nishino",
  "version": "1.0.0",
  "description": "A small reader for Tomotoshi Nishino's selection of papers on tensor networks",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run lint",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "prettier --check --plugin-search-dir=. --ignore-path .gitignore . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. --ignore-path .gitignore ."
  },
  "author": "Giacomo Giudice",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "fuse.js": "^6.4.6",
    "glob": "^7.2.0",
    "html-entities": "^2.3.3",
    "katex": "^0.15.0",
    "node-fetch": "^3.0.0",
    "svelte": "^3.44.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "postcss": "^8.3.11",
    "posthtml": "^0.16.5",
    "posthtml-include": "^1.7.2",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.43.4",
    "svelte-preprocess": "^4.9.8",
    "vite": "^2.6.13"
  },
  "browserslist": [
    "last 3 version",
    "> 0.25%",
    "not dead"
  ]
}
