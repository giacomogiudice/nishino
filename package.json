{
  "name": "nishino",
  "version": "1.0.0",
  "description": "A small reader for Tomotoshi Nishino's selection of papers on tensor networks",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run lint",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "prettier --check --plugin-search-dir=. --ignore-path .gitignore . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. --ignore-path .gitignore .",
    "db-update": "node --env-file=.env functions/util/setup.js"
  },
  "author": "Giacomo Giudice",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.5.3",
    "autoprefixer": "^10.4.21",
    "cheerio": "^1.1.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-svelte": "^2.46.1",
    "fuse.js": "^6.6.2",
    "glob": "^10.4.5",
    "html-entities": "^2.6.0",
    "katex": "^0.16.22",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "posthtml": "^0.16.6",
    "posthtml-include": "^1.7.4",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "redis": "^4.7.1",
    "sass": "^1.89.2",
    "svelte": "^3.59.2",
    "svelte-preprocess": "^5.1.4",
    "vite": "^4.5.14"
  },
  "browserslist": [
    "last 3 version",
    "> 0.25%",
    "not dead"
  ]
}
